; ModuleID = 'bril_llvm'
source_filename = "bril_llvm"

@0 = private unnamed_addr constant [14 x i8] c"Hi, I am %d.\0A\00", align 1

define i32 @my_main() {
entry:
  %0 = alloca i32
  %1 = getelementptr inbounds i32, i32* %0, i32 0
  store i32 10, i32* %1
  %2 = load i32, i32* %1
  %3 = call i32 @printf(i8* getelementptr inbounds ([14 x i8], [14 x i8]* @0, i32 0, i32 0), i32 %2)
  ret i32 0
}

declare i32 @printf(i8*, i32)
